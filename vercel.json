{
    "builds" :[{
        "src": "QuickTalk/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize" : "15mb", "runtime" : "python3.9" }
    }],
    "routes" :[
        {
            "src": "/(.*)",
            "dest" : "QuickTalk/wsgi.py"
        }
    ],
    "hooks": {
      "build": "cp static/db.sqlite3 /tmp/db.sqlite3"
    }
    }